\subsection{Inverse trigonometric functions}
Trigonometric functions in their conventional sense are not one-to-one since the function values repeat several times. So, in order to define inverse trigonometric functions, we must restrict the domains of our trigonometric functions.
\begin{itemize}
\item{$\sin^{-1}(x) = \arcsin(x)$
	\begin{itemize}
	\item $y = \sin^{-1}(x)$ is equivalent to $x = \sin(y)$
	\item The domain of $y=\sin^{-1}(x)$ is $-1 \leq x \leq 1$
	\item The range of $y=\sin^{-1}(x)$ is $-\dfrac{\pi}{2} \leq y \leq \dfrac{\pi}{2}$
	\end{itemize}}
\item{$\cos^{-1}(x) = \arccos(x)$
	\begin{itemize}
	\item $y = \cos^{-1}(x)$ is equivalent to $x = \cos(y)$
	\item The domain of $y=\cos^{-1}(x)$ is $-1 \leq x \leq 1$
	\item The range of $y=\cos^{-1}(x)$ is $0 \leq y \leq \pi$
	\end{itemize}}
\newpage
\item{$\tan^{-1}(x) = \arctan(x)$
	\begin{itemize}
	\item $y = \tan^{-1}(x)$ is equivalent to $x = \tan(y)$
	\item The domain of $y=\tan^{-1}(x)$ is $-\infty < x < \infty$
	\item The range of $y=\tan^{-1}(x)$ is $-\dfrac{\pi}{2} < y < \dfrac{\pi}{2}$
	\end{itemize}}
\end{itemize}


\subsection{Trigonometric identities}
\textbf{Pythagorian Trigonometric Identities}
\begin{itemize}
\item $\sin^2(x) + \cos^2(x) = 1$ 
\item $\tan^2(x) + 1 = \sec^2(x)$ 
\item $\cot^2(x) + 1 = \csc^2(x)$
\end{itemize}

\textbf{Addition/Subtraction Formulas}
\begin{itemize}
\item $\sin(x+y) = \sin(x) \cos(y) + \cos(x)\sin(y)$ 
\item $\cos(x+y) = \cos(x) \cos(y) - \sin(x) \sin(y) $
\item $\sin(x-y) = \sin(x) \cos(y) - \cos(x) \sin(y) $ 
\item $\cos(x-y) = \cos(x) \cos(y) + \sin(x) \sin(y) $
\end{itemize}

\textbf{Double-Angle Formulas}
\begin{itemize}
\item $\sin(2x) = 2\sin x \cos x $
\item $\cos(2x) = \cos^2 x - \sin^2 x$
\end{itemize}




