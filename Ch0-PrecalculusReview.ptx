\section{Transformation of Functions}
 
 \subsection{Shifts in the $x$ and $y$ Directions}
 For $k>0$ and some function $f(x)$, the graph of $f(x)$ is transformed by:
 \begin{itemize}
 \item Vertical shift up by $k$ units: $y = f(x) + k$
 \item Vertical shift down by $k$ units: $y = f(x) - k$
 \item Horizontal shift to the left by $k$ units: $y = f(x+k)$
 \item Horizontal shift to the right by $k$ units: $y = f(x-k)$
 \end{itemize}
 
 \subsection{Scalings in the $x$ and $y$ Directions}
 For $c>1$ and some function $f(x)$, the graph of $f(x)$ is transformed by:
 \begin{itemize}
 \item Vertical stretch by a factor of $c$: $y = cf(x)$
 \item Vertical shrink by a factor of $c$: $y = \dfrac{1}{c}f(x)$
 \item Horizontal shrink by a factor of $c$: $y = f(cx)$
 \item Horizontal stretch by a factor of $c$: $y = f\left(\dfrac{x}{c}\right)$
 \end{itemize}
 
 \subsection{Reflections about the $x-$ and $y-$axis}
 For some function $f(x)$,
 \begin{itemize}
 \item Reflect about the $x-$axis: $y = -f(x)$
 \item Reflect about the $y-$axis: $y = f(-x)$
 \end{itemize}
 
\section{Inverse Functions}
For a one-to-one function $f(x)$ with domain $D$ and range $R$, there exists an \textbf{inverse function} of $f(x)$, $f^{-1}(x)$, with domain $R$ and range $D$ such that whenever $f(x) = y$, then $f^{-1}(y) = x$.
\begin{itemize}
\item{If the graph of $f$ has the point $(x,y)$, then the graph of $f^{-1}$ has the point $(y,x)$.}
\item{A composite of the function with its inverse (or vice versa) yields the independent variable.
	\begin{itemize}
	\item[$\circ$] $f^{-1}(f(x)) = x$
	\item[$\circ$] $f(f^{-1}(x)) = x$
	\end{itemize}}
\end{itemize}

\textit{NOTE: $f^{-1}$(x) is the inverse of $f(x)$. This is NOT the same as the reciprocal of $f(x)$ which is $(f(x))^{-1}$}.

\subsection{Finding an inverse function}
Suppose $y = f(x)$ is a one-to-one function. In order to find $f^{-1}(x)$:
\begin{enumerate}
\item{Interchange $x$ and $y$ ($x \rightarrow y$ and $y \rightarrow x$)}
\item{Solve the resulting equation for $y$}
\item{Let $y = f^{-1}(x)$}
\end{enumerate}

\section{Exponential Functions}

%\subsection{Properties of exponential functions}
An \textbf{exponential function} is a function of the form $f(x) = a^x$ where $a > 0$ is a real number called the \textbf{base}. 

The \textbf{natural exponential function}, $f(x) = e^x$, has base $e$, a non-terminating, non-repeating number ($2.718281...$). 

If $f(x) = a^x$, with $a>0$, then: 
\begin{itemize}
\item The domain of $f(x)$ is $(-\infty,\infty)$
\item The range of $f(x)$ is $(0,\infty)$
\item The graph of $f(x)$ has a horizontal asymptote at $y = 0$
\item The graph contains the point $(0,1)$
\item If $a > 1$, the function is always increasing
\item If $0<a<1$, the function is always decreasing 
\end{itemize}

\begin{figure}[h!]
	\begin{center}
	\includegraphics[height=2.5in,width=2.5in]{ExponentialFunction}
	\end{center}
\end{figure}

\subsection{Laws of Exponents}
For any positive integers $m$ and $n$ and any real numbers $a$ and $b$,
\begin{itemize}\begin{multicols}{2}
\item $a^n = a \cdot a \cdots a$ ($n$ times)
\item $a^0 = 1$, $a \neq 0$
\item $a^{-n} = \dfrac{1}{a^n}$, $a \neq 0$
\item $a^m \cdot a^n = a^{m+n}$
\item $\dfrac{a^m}{a^n} = a^{m-n}$
\item $\left(a^m\right)^n = a^{m\cdot n}$
\item $\left(a \cdot b\right)^n = a^n \cdot b^n$
\item $a^{m/n} = \sqrt[n]{a^m} = (\sqrt[n]{a})^m$
\item $\left(\dfrac{a}{b}\right)^n = \dfrac{a^n}{b^n}$
\item $\left(\dfrac{a}{b}\right)^{-n} = \left(\dfrac{b}{a}\right)^n = \dfrac{b^n}{a^n}$
\end{multicols}\end{itemize}
Common errors using exponents:
\begin{itemize}
\item $(a+b)^n \neq a^n + b^n$
\item $(-a)^n \neq -a^n$
\end{itemize}


\section{Logarithmic Functions}

%\subsection{Properties of logarithmic functions}
The inverse of an exponential function with base $a$, $y = a^x$, is a \textbf{logarithmic function} with base $a$, $y = \log_{a}(x)$.

The inverse of the natural exponential function, $y = e^x$, is the \textbf{natural logarithmic function}, $y = \ln(x)$.

If $y = \log_{a}(x)$,
\begin{itemize}
\item{This is equivalent to $x = a^y$}
\item{The domain of this function is $(0, \infty)$}
\item{The range of this function is $(-\infty, \infty)$}
\item{The graph of this function has a vertical asymptote at $x = 0$}
\item{The graph contains the point $(1,0)$}
\end{itemize}

Since logarithmic and exponential functions are inverses of each other, then
\begin{itemize}
\item{$\log_{a}(a^x) = x = a^{\log_a(x)}$}
\item{$\ln(e^x) = x = e^{\ln(x)}$}
\end{itemize}

